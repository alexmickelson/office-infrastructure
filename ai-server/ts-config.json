{
  "TCP": {
    "443": {
      "HTTPS": true
    },
    "9090": {
      "HTTPS": true
    }
  },
  "Web": {
    "${TS_CERT_DOMAIN}:443": {
      "Handlers": {
        "/": {
          "Proxy": "http://127.0.0.1:8080"
        }
      }
    },
    "${TS_CERT_DOMAIN}:9090": {
      "Handlers": {
        "/": {
          "Proxy": "http://host.docker.internal:9090"
        }
      }
    },
    "${TS_CERT_DOMAIN}:3000": {
      "Handlers": {
        "/": {
          "Proxy": "http://host.docker.internal:3000"
        }
      }
    }
  },
  "AllowFunnel": {
    "${TS_CERT_DOMAIN}:443": false
  }
}